class Solution {
    public int countOdds(int low, int high) {
        // brute force
        // int count=0;
        // for(int i=low;i<=high;i++){
        //     if(i%2!=0){
        //         count++;
        //     }
        // }
        // return count;

        //  optimal 
        // from observation
        if(low%2==0 && high%2==0){
            return (high-low)/2;
        }else if(low%2!=0 && high%2!=0){
            // if both odd +1
            return (high-low)/2+1;
        }
        // ek odd ya even
        return (high-low)/2+1;
    }
}
